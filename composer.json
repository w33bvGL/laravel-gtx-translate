{
    "name": "w33bvgl/laravel-gtx-translate",
    "description": "Free Google Translate (GTX) API scraper for Laravel",
    "license": "GPL-3.0-or-later",
    "type": "library",
    "keywords": [
        "laravel",
        "translate",
        "google",
        "gtx",
        "scraper"
    ],
    "autoload": {
        "psr-4": {
            "W33bvgl\\GtxTranslate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "W33bvgl\\GtxTranslate\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "authors": [
        {
            "name": "Vahe Sargsyan",
            "email": "w33bv.gl@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.17",
        "guzzlehttp/guzzle": "^7.9",
        "psr/log": "^3.0",
        "symfony/browser-kit": "^7.2",
        "symfony/dom-crawler": "^7.2",
        "symfony/http-client": "^7.2",
        "php-webdriver/webdriver": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "phpstan/phpstan": "^2.1",
        "orchestra/testbench": "^7.50",
        "laravel/pint": "^1.19",
        "monolog/monolog": "^2.10"
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare",
            "./vendor/bin/pint .",
            "./vendor/bin/phpstan analyse --memory-limit=512M",
            "./vendor/bin/phpunit tests"
        ],
        "pint": [
            "./vendor/bin/pint ."
        ],
        "analyse": [
            "./vendor/bin/phpstan analyse --configuration=.phpstan.neon --memory-limit=512M"
        ],
        "test": [
            "./vendor/bin/phpunit tests"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "lint": [
            "@php vendor/bin/pint --ansi",
            "@php vendor/bin/phpstan analyse --verbose --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "W33bvgl\\GtxTranslate\\GtxTranslateServiceProvider"
            ],
            "aliases": {
                "Translate": "W33bvgl\\GtxTranslate\\Facades\\Translate"
            }
        },
        "funding": {
            "github": [
                "w33bvGL"
            ]
        }
    },
    "minimum-stability": "stable"
}
