{
  "name": "animik/google-translate-scraper",
  "description": "google-translate-scraper",
  "type": "library",
  "autoload": {
    "psr-4": {
      "AniMik\\MalCrawler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AniMik\\MalCrawler\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Vahe Sargsyan",
      "email": "w33bv.gl@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "spatie/laravel-package-tools": "^1.17",
    "guzzlehttp/guzzle": "^7.9",
    "psr/log": "^3.0",
    "symfony/browser-kit": "^7.2",
    "symfony/dom-crawler": "^7.2",
    "symfony/http-client": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "orchestra/testbench": "^7.49",
    "laravel/pint": "^1.19"
  },
  "extra": {
    "laravel": {
      "providers": [
        "AniMik\\MalCrawler\\Providers\\MalCrawlerServiceProvider"
      ],
      "aliases": {
        "AnimeCrawler": "AniMik\\MalCrawler\\Facades\\AnimeCrawler"
      }
    },
    "funding": {
      "github": ["w33bvGL"]
    }
  },
  "minimum-stability": "stable"
}
