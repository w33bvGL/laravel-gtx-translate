{
  "name": "anidzen/google-translate-scraper",
  "description": "google-translate-scraper",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Anidzen\\GoogleTranslateScraper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Anidzen\\GoogleTranslateScraper\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Vahe Sargsyan",
      "email": "w33bv.gl@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "spatie/laravel-package-tools": "^1.17",
    "guzzlehttp/guzzle": "^7.9",
    "psr/log": "^3.0",
    "symfony/browser-kit": "^7.2",
    "symfony/dom-crawler": "^7.2",
    "symfony/http-client": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "phpstan/phpstan": "^2.1",
    "orchestra/testbench": "^7.50",
    "laravel/pint": "^1.19",
    "monolog/monolog": "^2.10"
  },
  "scripts": {
    "post-autoload-dump": [
      "./vendor/bin/pint .",
      "./vendor/bin/phpstan analyse --memory-limit=512M"
    ],
    "pint": [
      "./vendor/bin/pint ."
    ],
    "analyse": [
      "./vendor/bin/phpstan analyse --configuration=.phpstan.neon --memory-limit=512M"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "GoogleTranslateScraperServiceProvider"
      ],
      "aliases": {
        "TextTranslator": "TextTranslator"
      }
    },
    "funding": {
      "github": ["w33bvGL"]
    }
  },
  "minimum-stability": "stable"
}
